<h1>New subcategory</h1>

<% form_for(@subcategory) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :child_id %><br />
    <%= f.text_field :child_id %>
  </p>
  <p>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', subcategories_path %>
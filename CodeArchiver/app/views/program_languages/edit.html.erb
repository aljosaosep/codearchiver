<h1>Editing program_language</h1>

<% form_for(@program_language) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :language_name %><br />
    <%= f.text_field :language_name %>
  </p>
  <p>
    <%= f.label :reservered_words %><br />
    <%= f.text_area :reservered_words %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @program_language %> |
<%= link_to 'Back', program_languages_path %>
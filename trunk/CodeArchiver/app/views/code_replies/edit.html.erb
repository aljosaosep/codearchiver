<h1>Editing code_reply</h1>

<% form_for(@code_reply) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :child_id %><br />
    <%= f.text_field :child_id %>
  </p>
  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @code_reply %> |
<%= link_to 'Back', code_replies_path %>
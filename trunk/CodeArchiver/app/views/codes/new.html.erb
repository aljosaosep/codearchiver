
<h1>New code</h1>

<% form_for(@code) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  
  <p>
    <%= f.label :code_body %><br />
    <%= f.text_area :code_body %>
  </p>
  
  <p>
    <%= f.label :tag %><br />
    <%= f.text_field :tag %>
  </p>
  
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.label :shortdescription %><br />
    <%= f.text_area :shortdescription %>
  </p>
 
  <p>
  	User id <br />
    <!-- <%= f.label :user_id %><br /> -->
    <%= f.text_field :user_id %>
  </p>
  
  <p>
  	Program Language id<br />
    <!--<%= f.label :program_language %>-->
    <%= f.text_field :program_language_id %>
  </p>
  
  <p>
  	Category id <br />
    <!--<%= f.label :category %>-->
    <%= f.text_field :category_id %>
  </p>
  
  <p>
  	Type id
    <%= f.label :type %><br />
    <%= f.text_field :type_id %>
  </p>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
  
<% end %>
